{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "setupFiles": ["./test/env-setup.ts"],
  "globalSetup": "./test/setup.ts",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
